{
  "name": "easy-postgresql",
  "version": "1.3.8",
  "description": "Simple ORM for postgresql",
  "main": "dist/index.js",
  "module": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "webpack --mode=production --config webpack.config.js",
    "build:esm": "rollup --config rollup.config.js",
    "build:ts": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json"
  },
  "dependencies": {
    "pg": "^8.11.0",
    "prompt-sync": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "babel-loader": "^9.1.3",
    "glob": "^10.3.3",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^1.4.14",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [],
  "author": "DimTzilop",
  "license": "ISC"
}